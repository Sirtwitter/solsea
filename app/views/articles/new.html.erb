<div class="container">
  <h3 class="title">Agregar Articulo</h3>
  <hr>


  <%= simple_form_for @article do |f| %>
  <%= f.input :title %>
  <%= f.input :location %>
  <%= f.cktext_area :description %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.button :submit, class: "btn btn-info title" %>
  <% end %>

  <hr>

</div>
